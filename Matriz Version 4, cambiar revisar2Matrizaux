    public boolean revisar2Matrizaux(ArrayList<String> m1,ArrayList<ArrayList<Integer>> m2){
        if (m1.size()==m2.size()){
            String[] l = m1.get(0).split(",");
            if(l.length==m2.get(0).size()){
                return true;
            }
            else{
                return false;
            }
        }
        else{
            return false;
        }
    }
    
    
    
